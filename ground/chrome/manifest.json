{
	"manifest_version": 2,
	"name": "Hello World",
	"version": "2.1",
	"minimum_chrome_version": "23",
	"icons": {
		"16": "icon_16.png",
		"128": "icon_128.png"
	},
	"app": {
		"background": {
			"scripts": ["main.js"]
		}
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions": [
		"serial", 
		"https://maps.googleapis.com/*",
		"http://maps.google.com/*"
	]
}
